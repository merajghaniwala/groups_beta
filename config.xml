id          = "com.webmaxinfotech.parcelmgt"
version     = "1.0.0"
versionCode = "10" >

	<!-- versionCode is optional and Android only -->

	<preference name="phonegap-version" value="3.3.0" />
	
	<name>Groups Demo</name>
	<description>Groups Demo</description>
	<author href="http://webmaxinfotech.com" email="merajgmemon@gmail.com">Meraj Ghaniwala</author>
	
	<feature name="SocialSharing">
